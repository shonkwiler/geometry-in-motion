  FormBox[
   RowBox[{
    RowBox[{"\[Alpha]", "(", "t", ")"}], "=", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["y", "0"], "t"}], ",", 
      SubscriptBox["y", "0"]}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ce3d27a4-0271-4b72-9433-e6c274441a81"],
