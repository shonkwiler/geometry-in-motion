Cell[TextData[{
 "Consider the hyperbolic plane ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalH]", "2"], TraditionalForm]],
  ExpressionUUID->"17893820-4e13-4990-9c12-5d24c3eea643"],
 ", realized in the ",
 ButtonBox["Poincar\[EAcute] half-plane model",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://en.wikipedia.org/wiki/Poincar\[EAcute]_half-plane_model"], 
    None},
  ButtonNote->
   "https://en.wikipedia.org/wiki/Poincar\[EAcute]_half-plane_model"],
 ". We can interpret this as the upper half-plane ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"x", ",", "y"}], ")"}], " ", "\[Element]", 
     RowBox[{
      SuperscriptBox["\[DoubleStruckCapitalR]", "2"], ":", " ", 
      RowBox[{"y", " ", ">", " ", "0"}]}]}], "}"}], TraditionalForm]],
  ExpressionUUID->"fbc68343-cde9-438d-8b38-19c8772f0a3f"],
 " together with the Riemannian metric ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", 
     SuperscriptBox["s", "2"]}], "=", 
    RowBox[{
     FractionBox["1", 
      SuperscriptBox["y", "2"]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"\[DifferentialD]", 
        SuperscriptBox["x", "2"]}], "+", 
       RowBox[{"\[DifferentialD]", 
        SuperscriptBox["y", "2"]}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"37354e90-8b33-4021-bf27-27a4dd3ab553"],
 "."
}], "Text",
 CellChangeTimes->{{3.957885847576595*^9, 3.957886044998527*^9}, {
  3.957886426446733*^9, 3.957886443958366*^9}, {3.957886478531139*^9, 
  3.9578864874481583`*^9}, {3.957953633380362*^9, 
  3.957953639739069*^9}},ExpressionUUID->"5b143438-b125-48e2-ad8a-\
df08f5e198b9"],

Cell[TextData[{
 "In other words, if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", "=", 
     RowBox[{
      SubscriptBox["u", "1"], 
      RowBox[{
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "x"}]], 
       RowBox[{"+", 
        SubscriptBox["u", "2"]}]}], 
      FractionBox["\[PartialD]", 
       RowBox[{"\[PartialD]", "y"}]]}]}], ",", 
    RowBox[{"V", "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["v", "1"], 
       RowBox[{
        FractionBox["\[PartialD]", 
         RowBox[{"\[PartialD]", "x"}]], 
        RowBox[{"+", 
         SubscriptBox["v", "2"]}]}], 
       FractionBox["\[PartialD]", 
        RowBox[{"\[PartialD]", "x"}]]}], "\[Element]", 
      RowBox[{
       SubscriptBox["T", 
        RowBox[{"(", 
         RowBox[{"x", ",", "y"}], ")"}]], 
       SuperscriptBox["\[DoubleStruckCapitalH]", "2"]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"97a2bd0c-f5d7-4941-8cf3-380893f9a8fa"],
 ", then\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"U", ",", "V"}], "\[RightAngleBracket]"}], 
     RowBox[{"(", 
      RowBox[{"x", ",", "y"}], ")"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["u", "1"], 
        SubscriptBox["v", "1"]}], 
       SuperscriptBox["y", "2"]], "+", 
      FractionBox[
       RowBox[{
        SubscriptBox["u", "2"], 
        SubscriptBox["v", "2"]}], 
       SuperscriptBox["y", "2"]]}], "=", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox["y", "2"]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["u", "1"], 
         SubscriptBox["v", "1"]}], "+", 
        RowBox[{
         SubscriptBox["u", "2"], 
         SubscriptBox["v", "2"]}]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"08f101af-cbd7-43a1-89b3-a0376c72eead"],
 "."
}], "Text",
 CellChangeTimes->{{3.957886547322152*^9, 3.957886676086685*^9}, {
  3.957953651238912*^9, 
  3.957953691871023*^9}},ExpressionUUID->"60eb1b8a-739e-45b6-bd11-\
b73ad6cd44f3"],

Cell[TextData[{
 "Therefore, if ",
 Cell[BoxData[
  FormBox["\[Del]", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2ddfe706-2e2d-483f-874e-23721ec90276"],
 " is the ",
 ButtonBox["Levi-Civita connection",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://en.wikipedia.org/wiki/Levi-Civita_connection"], None},
  ButtonNote->"https://en.wikipedia.org/wiki/Levi-Civita_connection"],
 " associated with this Riemannian metric, one can check that, using the \
notation ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", "=", 
    FractionBox["\[PartialD]", 
     RowBox[{"\[PartialD]", "x"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a2cf7633-1a5b-46fd-8565-a745d5f0bdc4"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Y", "=", 
    FractionBox["\[PartialD]", 
     RowBox[{"\[PartialD]", "y"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f560d67a-9591-439c-93dd-7339003d342a"],
 ",\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Del]", "X"], "X"}], "=", 
    RowBox[{
     FractionBox["1", "y"], "Y"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "bf3be88b-0019-4ff6-9373-6d4c9ad2c7a2"],
 ",\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Del]", "X"], "Y"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Del]", "Y"], "X"}], "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "y"]}], "X"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "169c59a2-cc86-41f8-a37b-4102e357c381"],
 ", \t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Del]", "Y"], "Y"}], "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", "y"]}], "Y"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e446b184-6a2a-47fc-820b-e01abcb09989"],
 "."
}], "Text",
