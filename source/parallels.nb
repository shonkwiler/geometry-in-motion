Cell[TextData[{
 "Now, consider the curve ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Alpha]", "(", "t", ")"}], "=", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SubscriptBox["y", "0"], "t"}], ",", 
      SubscriptBox["y", "0"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "7e11135e-6041-4ddf-8c88-cd368ed03302"],
 " for some fixed ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "0"], ">", "0"}], TraditionalForm]],ExpressionUUID->
  "429a4abc-c6f3-4c75-830a-cec82b18498f"],
 ". This is a ",
 ButtonBox["horocycle",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://en.wikipedia.org/wiki/Horocycle"], None},
  ButtonNote->"https://en.wikipedia.org/wiki/Horocycle"],
 " centered at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", "\[Infinity]"}], TraditionalForm]],ExpressionUUID->
  "dd778ba4-074a-4c5a-b988-4bfc95666f75"],
 ". It\[CloseCurlyQuote]s parametrized by arclength since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Alpha]", "'"}], 
     RowBox[{"(", "t", ")"}]}], "=", 
    RowBox[{
     SubscriptBox["y", "0"], "X"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fdff89ab-0305-4140-8e0e-19f7b46c9bcd"],
 ", which is a unit vector since ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DoubleVerticalBar]", 
     RowBox[{
      SubscriptBox["y", "0"], "X"}], 
     SuperscriptBox["\[DoubleVerticalBar]", "2"]}], "=", 
    RowBox[{
     SubscriptBox[
      RowBox[{"\[LeftAngleBracket]", 
       RowBox[{
        RowBox[{
         SubscriptBox["y", "0"], "X"}], ",", 
        RowBox[{
         SubscriptBox["y", "0"], "X"}]}], "\[RightAngleBracket]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["y", "0"], "t"}], ",", 
        SubscriptBox["y", "0"]}], ")"}]], "=", 
     RowBox[{
      FractionBox[
       SubsuperscriptBox["y", "0", "2"], 
       SubsuperscriptBox["y", "0", "2"]], "=", "1"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d1d0e71a-0da5-40e9-a025-9f63d5806410"],
 ". "
}], "Text",
 CellChangeTimes->{{3.95788697445362*^9, 3.957887018015984*^9}, {
  3.957887082216547*^9, 3.95788735672007*^9}, {3.9578887638551207`*^9, 
  3.957888779728751*^9}, {3.9578894187311487`*^9, 3.95788941913698*^9}, {
