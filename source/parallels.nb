Cell[TextData[{
 "To produce the final animation, we\[CloseCurlyQuote]re going to transform \
the curve ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7fa0a8fe-dc95-4339-a8a8-53a86dc82921"],
 " (which remember was a horocycle centered at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", "\[Infinity]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d7ab264f-a746-4598-9244-a2e3664d384a"],
 ") to a horocycle centered at a finite point on the ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "598d9778-3bf1-4f47-acb7-ecc244c2016b"],
 "-axis. "
}], "Text",
 CellChangeTimes->{{3.9578893761342483`*^9, 3.957889394442977*^9}, {
  3.957889426312237*^9, 3.957889463254731*^9}, {3.9579540781946793`*^9, 
  3.9579541062280073`*^9}},ExpressionUUID->"977a14f4-8935-4f7b-a4af-\
68c46144c167"],

Cell[TextData[{
 "Recall that the ",
 ButtonBox["Cayley transform",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://en.wikipedia.org/wiki/Cayley_transform"], None},
  ButtonNote->"https://en.wikipedia.org/wiki/Cayley_transform"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "z", ")"}], "=", 
    FractionBox[
     RowBox[{"z", "-", "\[ImaginaryI]"}], 
     RowBox[{"z", "+", "\[ImaginaryI]"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b18c3c36-b6f8-44d4-8f7d-5da8978e290f"],
 " maps the upper half-plane model to the Poincar\[EAcute] disk model, and of \
course its inverse ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["f", 
      RowBox[{"-", "1"}]], "(", "z", ")"}], "=", 
    RowBox[{"\[ImaginaryI]", 
     FractionBox[
      RowBox[{"1", "+", "z"}], 
      RowBox[{"1", "-", "z"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "46e9d8e6-78e2-4496-b25e-b85440b9355c"],
 " goes the other way:"
}], "Text",
 CellChangeTimes->{{3.957889542366501*^9, 3.957889634364962*^9}, {
  3.957889690216914*^9, 
  3.957889750267765*^9}},ExpressionUUID->"aee1f3c6-b405-4e43-96cb-\
af9e7ad49143"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Cayley", "[", "z_", "]"}], ":=", 
   FractionBox[
    RowBox[{"z", "-", "I"}], 
    RowBox[{"z", "+", "I"}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"CayleyInverse", "[", "z_", "]"}], ":=", 
   RowBox[{"I", 
    FractionBox[
     RowBox[{"1", "+", "z"}], 
     RowBox[{"1", "-", "z"}]]}]}], ";"}]}], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.957889678291525*^9, 3.95788967927828*^9}, {
  3.957889752502923*^9, 3.957889767123948*^9}},
 CellLabel->
  "In[160]:=",ExpressionUUID->"7b91cb90-c875-4d3a-858e-af58db28a664"],

